        <Button Content="Start Game" Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="buttonLaunchDF" VerticalAlignment="Top" Width="75" Click="buttonLaunchDF_Click" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="12,41,0,0" Name="comboSaveSelect" VerticalAlignment="Top" Width="180" />
